{
  "name": "dagger-backend",
  "version": "0.1.0",
  "type": "module",
  "description": "Hono API server for Dagger network inspection",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --noEmit",
    "start": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:costing": "vitest run src/services/costing/",
    "test:costing:integration": "vitest run src/services/costing/*.integration.test.ts",
    "test:services": "vitest run src/services/*.test.ts",
    "test:coverage": "vitest run --coverage",
    "build:wasm": "cd ../cli && wasm-pack build --target nodejs --out-dir ../backend/pkg",
    "build:wasm:release": "cd ../cli && wasm-pack build --release --target nodejs --out-dir ../backend/pkg"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "effect": "^3.19.13",
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@types/node": "^22",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
